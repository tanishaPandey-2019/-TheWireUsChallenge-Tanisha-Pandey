#include <iostream>
using namespace std;


int main()
{
	int numI;
	cin >> numI;
	string s[100];
	for (int i = 0; i < numI; i++)
	{
		cin >> s[i];
	}
	

	int numQ;
	cin >> numQ;
	string str[100];
	for (int i = 0; i < numQ; i++)
	{
		cin >> str[i];
	}


	
	for (int i = 0; i < numQ; i++)
	{
		
		int count = 0;
		for (int j = 0; j < numI; j++)
		{
			if (str[i] == s[j])
			{
				count++;
			}

		}
		cout << count << endl;
	}
	return 0;
	

}
