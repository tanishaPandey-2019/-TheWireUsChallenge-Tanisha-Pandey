#include <iostream>
using namespace std;

int main()
{
	int n;
	cin >> n;

	int arr[100];
	for (int i = 0; i < n; i++)
	{
		cin >> arr[i];
	}

	cout << 1 << " ";
	int count = 1;
	for (int i = 1; i < n; i++)
	{
		if (arr[i-1] <= arr[i])
		{
			count ++;
			cout << count << " ";
		}
		else
		{
			cout << 1 << " ";
		}
	}
	cout << "END";

	return 0;
}
